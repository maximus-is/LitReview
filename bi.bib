@article{Alizadeh2002,
abstract = {We propose using the price range in the estimation of stochastic volatility models. We show theoretically, numerically, and empirically that range-based volatility proxies are not only highly efficient, but also approximately Gaussian and robust to microstructure noise. Hence range-based Gaussian quasi-maximum likelihood estimation produces highly efficient estimates of stochastic volatility models and extractions of latent volatility. We use our method to examine the dynamics of daily exchange rate volatility and find the evidence points strongly toward two-factor models with one highly persistent factor and one quickly mean-reverting factor. CR  - Copyright \&\#169; 2002 American Finance Association},
annote = {This considers range-based estimation instead of squared returns to fit a stochastic volatility model, extending beyond Geometric Brownian Motion to Stochastic Volatility. They do not examine intraday data.
        
They show that log range is approximately normal, yet fail to conduct tests of normality on the results, merely suggesting that a visual comparison with a normal distribution is suggestive of normality, as are skewness of 0.17 and kurtosis of 2.80. 
        
The authors find that simple Geometric Brownian Motion is not sufficient to explain the autocorrelation of volatility and its variance as found in foreign exchange data.
        
Range based estimators are more effcient than squared-return estimators in the presence of bid-ask bounce, as the observed value may be higher than the true value when the price realises the ask price, and the opposite for the bid price.},
author = {Alizadeh, Sassan and Brandt, Michael W and Diebold, Francis X},
doi = {10.2307/2697772},
file = {:C$\backslash$:/Users/maximus/Dropbox/usyd/Thesis/Mendeley//Alizadeh, Brandt, Diebold - 2002 - Range-Based Estimation of Stochastic Volatility Models.pdf:pdf},
issn = {00221082},
journal = {The Journal of Finance},
month = jun,
number = {3},
pages = {1047--1091},
publisher = {Wiley for the American Finance Association},
title = {{Range-Based Estimation of Stochastic Volatility Models}},
url = {http://www.jstor.org/stable/2697772},
volume = {57},
year = {2002}
}
@article{Andersen2003,
abstract = {We provide a framework for integration of high-frequency intraday data into the measurement, modeling, and forecasting of daily and lower frequency return volatilities and return distributions. Building on the theory of continuous-time arbitrage-free price processes and the theory of quadratic variation, we develop formal links between realized volatility and the conditional covariance matrix. Next, using continuously recorded observations for the Deutschemark/Dollar and Yen/Dollar spot exchange rates, we find that forecasts from a simple long-memory Gaussian vector autoregression for the logarithmic daily realized volatilities perform admirably. Moreover, the vector autoregressive volatility forecast, coupled with a parametric lognormal-normal mixture distribution produces well-calibrated density forecasts of future returns, and correspondingly accurate quantile predictions. Our results hold promise for practical modeling and forecasting of the large covariance matrices relevant in asset pricing, asset allocation, and financial risk management applications. CR - Copyright \&\#169; 2003 The Econometric Society},
author = {Andersen, Torben G and Bollerslev, Tim I M and Diebold, Francis X and Labys, Paul},
doi = {10.2307/3082068},
file = {:C$\backslash$:/Users/maximus/Dropbox/usyd/Thesis/Mendeley/Andersen et al. - 2003 - Modeling and Forecasting Realized Volatility.pdf:pdf},
issn = {00129682},
journal = {Econometrica},
month = mar,
number = {2},
pages = {579--625},
publisher = {The Econometric Society},
title = {{Modeling and Forecasting Realized Volatility}},
url = {http://www.jstor.org/stable/3082068},
volume = {71},
year = {2003}
}
@article{Andersen2001,
abstract = {Using high-frequency data on deutschemark and yen returns against the dollar, we construct model-free estimates of daily exchange rate volatility and correlation that cover an entire decade. Our estimates, termed realized volatilities and correlations, are not only model-free, but also approximately free of measurement error under general conditions, which we discuss in detail. Hence, for practical purposes, we may treat the exchange rate volatilities and correlations as observed rather than latent. We do so, and we characterize their joint distribution, both unconditionally and conditionally. Noteworthy results include a simple normality-inducing volatility transformation, high contemporaneous correlation across volatilities, high correlation between correlation and volatilities, pronounced and persistent dynamics in volatilities and correlations, evidence of long-memory dynamics in volatilities and correlations, and remarkably precise scaling laws under temporal aggregation. CR - Copyright \&\#169; 2001 American Statistical Association},
author = {Andersen, Torben G and Bollerslev, Tim I M and Diebold, Francis X and Labys, Paul},
doi = {10.2307/2670339},
file = {:C$\backslash$:/Users/maximus/Dropbox/usyd/Thesis/Mendeley//Andersen et al. - 2001 - The Distribution of Realized Exchange Rate Volatility.pdf:pdf},
issn = {01621459},
journal = {Journal of the American Statistical Association},
keywords = {forecasting},
month = mar,
number = {453},
pages = {42--55},
publisher = {American Statistical Association},
title = {{The Distribution of Realized Exchange Rate Volatility}},
url = {http://www.jstor.org/stable/2670339},
volume = {96},
year = {2001}
}
@article{Andersen2011,
abstract = {We extend the analytical results for reduced form realized volatility based forecasting in ABM (2004) to allow for market microstructure frictions in the observed high-frequency returns. Our results build on the eigenfunction representation of the general stochastic volatility class of models developed byMeddahi (2001). In addition to traditional realized volatility measures and the role of the underlying sampling frequencies, we also explore the forecasting performance of several alternative volatility measures designed to mitigate the impact of the microstructure noise. Our analysis is facilitated by a simple unified quadratic form representation for all these estimators. Our results suggest that the detrimental impact of the noise on forecast accuracy can be substantial. Moreover, the linear forecasts based on a simple-to-implement ‘average’ (or ‘subsampled’) estimator obtained by averaging standard sparsely sampled realized volatility measures generally perform on par with the best alternative robust measures.},
author = {Andersen, Torben G and Bollerslev, Tim I M and Meddahi, Nour},
doi = {10.1016/j.jeconom.2010.03.032},
file = {:C$\backslash$:/Users/maximus/Dropbox/usyd/Thesis/Mendeley/Andersen, Bollerslev, Meddahi - 2011 - Realized volatility forecasting and market microstructure noise.pdf:pdf},
issn = {03044076},
journal = {Journal of Econometrics},
keywords = {C14,C22,C52,Eigenfunction stochastic volatility models,G14,High-frequency data,Integrated volatility,Market microstructure noise,Realized volatility,Robust volatility measures,Volatility forecasting},
month = jan,
